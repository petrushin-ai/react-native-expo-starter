# Authentication Configuration
# Set to 'true' to enable authentication, 'false' to disable
EXPO_PUBLIC_AUTH_ENABLED=true

# Simple auth credentials (for development/bootstrap purposes)
EXPO_PUBLIC_AUTH_USERNAME=demo
EXPO_PUBLIC_AUTH_PASSWORD=demo

# Auth session timeout in milliseconds (optional)
EXPO_PUBLIC_AUTH_SESSION_TIMEOUT=86400000

# App configuration
EXPO_PUBLIC_APP_NAME=React Native Expo Bootstrap 