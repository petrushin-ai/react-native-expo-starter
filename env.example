# Authentication Configuration
# Set to 'true' to enable authentication, 'false' to disable
EXPO_PUBLIC_AUTH_ENABLED=false

# Default credentials for development/demo purposes
# These are only used when AUTH_ENABLED is true
EXPO_PUBLIC_AUTH_USERNAME=admin
EXPO_PUBLIC_AUTH_PASSWORD=password123

# Permissions Configuration
# Set to 'true' to enable specific permissions
EXPO_PUBLIC_PERMISSIONS_NOTIFICATIONS=true
EXPO_PUBLIC_PERMISSIONS_LOCATION=true

# Enable debug mode to show permission configuration and reset option
EXPO_PUBLIC_PERMISSIONS_DEBUG=false 